{
  "name": "Soundcasts",
  "version": "1.1.0",
  "description": "Turn SoundCloud profiles into podcasts.",
  "main": "index.js",
  "engines": {
    "node": "5.10.0"
  },
  "scripts": {
    "prestart": "rm -rf build && mkdir build && babel lib --out-dir build --source-maps --optional runtime",
    "start": "NODE_ENV=production node index.js",
    "cover": "NODE_ENV=test babel-node node_modules/.bin/babel-istanbul cover test/index.js",
    "test": "NODE_ENV=test babel-node test/index.js | tap-spec",
    "travis-test": "npm run cover && ((cat coverage/lcov.info | coveralls) || exit 0)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/L33T-KR3W/soundcasts-server.git"
  },
  "author": "L33T KR3W",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/L33T-KR3W/soundcasts-server/issues"
  },
  "homepage": "https://github.com/L33T-KR3W/soundcasts-server",
  "dependencies": {
    "babel-cli": "^6.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-async-to-bluebird": "^1.0.0",
    "babel-preset-es2015-node": "^5.0.2",
    "bluebird": "^3.1.1",
    "body-parser": "^1.13.2",
    "deep-extend": "^0.4.0",
    "express": "^4.13.1",
    "js2xmlparser": "^1.0.0",
    "moment": "^2.11.1",
    "request": "^2.58.0"
  },
  "devDependencies": {
    "babel-istanbul": "^0.5.9",
    "coveralls": "^2.11.4",
    "nine-track": "^3.0.1",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.1"
  }
}
