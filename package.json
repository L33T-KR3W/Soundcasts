{
  "name": "Soundcasts",
  "version": "2.0.0-1",
  "description": "Turn SoundCloud profiles into podcasts.",
  "main": "index.js",
  "engines": {
    "node": "6.x"
  },
  "author": "L33T KR3W",
  "contributors": [
    "Matthew Medal <matt.medal@gmail.com>",
    "Ryan Tsao <ryan.j.tsao@gmail.com>",
    "Scott Hardy <scott.the.hardy@gmail.com>"
  ],
  "repository": "git@github.com:L33T-KR3W/soundcasts-server.git",
  "homepage": "https://github.com/L33T-KR3W/soundcasts-server",
  "bugs": "https://github.com/L33T-KR3W/soundcasts-server/issues",
  "scripts": {
    "transpile": "BABEL_ENV=development babel src --out-dir build --source-maps",
    "prestart": "rm -rf build && mkdir build && npm run transpile",
    "start": "NODE_ENV=production node index.js",
    "cover": "BABEL_ENV=development NODE_ENV=test babel-node node_modules/istanbul/lib/cli.js cover test/index.js",
    "test": "BABEL_ENV=development NODE_ENV=test babel-node test/index.js | tap-spec",
    "travis-test": "npm run cover && ((cat coverage/lcov.info | coveralls) || exit 0)"
  },
  "dependencies": {
    "babel-cli": "6.x",
    "babel-plugin-transform-async-to-generator": "6.x",
    "babel-plugin-transform-es2015-destructuring": "6.x",
    "babel-plugin-transform-es2015-parameters": "6.x",
    "babel-plugin-transform-object-rest-spread": "6.x",
    "babel-preset-es2015-node": "6.x",
    "body-parser": "^1.15.1",
    "deep-extend": "^0.4.1",
    "express": "^4.13.4",
    "js2xmlparser": "^1.0.0",
    "moment": "^2.13.0",
    "request": "^2.72.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.9",
    "istanbul": "^1.0.0-alpha.2",
    "nine-track": "^3.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1"
  },
  "license": "MIT"
}
